citHeader("To cite package 'Literasee' in publications use:")

citEntry(entry = "Manual",
         title        = "{Literasee}: Assessment Literacy Content Development via R.",
         author       = "Damian W. Betebenner and Adam R. {Van Iwaarden}",
         year         = "2017",
         note         = "R package version 0.0-4.5",
         url          = "literacy.io",

         textVersion  = paste("Damian W. Betebenner and Adam R. Van Iwaarden (2017).",
                        "Literasee: Assessment Literacy Content Development via R.",
                        "(R package version 0.0-4.5)",
                        "URL: literasee.io"))
